import java.util.Scanner;

public class javabasics{
    
    public static String answerString=new String();
    public static String removeOccurencesInAStringUsingRecursion(String s1,String characterToBeRemoved,int idx){
        
        if(idx==s1.length()){          
            return " ";
        }
        answerString=removeOccurencesInAStringUsingRecursion(s1, characterToBeRemoved, idx+1);
        char currChar=s1.charAt(idx);
        if(currChar!=characterToBeRemoved.charAt(0)){
            return currChar+answerString;
        }else{
            return answerString;
        }
        

    }
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        String s1=sc.next();
        String characterToBeRemoved = sc.next();   
        System.out.println(removeOccurencesInAStringUsingRecursion(s1,characterToBeRemoved,0));
           
    }
}
